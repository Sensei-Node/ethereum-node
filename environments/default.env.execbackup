# comma separated values of Engine API nodes socket (ex. 10.0.0.1:8551,10.0.0.2:8551)
NODES=http://execution:8551

# port to be exposed. Default is 9090, if changed make sure to change docker-compose.yml file as well.
PORT=9090

# Possible values: TRACE DEBUG INFO WARN ERROR CRITICAL
LOG_LEVEL=info

# JWT of the Engine API nodes (secrets/jwt.hex value)
JWT_SECRET=

# In case you want to use a specific release
EB_RELEASE_TAG=v1.1.2

# For multiplexing purposes
EB_FCU_MAJORITY=0.4